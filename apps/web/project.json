{
  "name": "@temar/web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project @temar/web --web",
  "targets": {
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "@temar/web:build",
        "dev": true,
        "port": 5173,
        "hostname": "0.0.0.0"
      }
    },
    "build": {
      "dependsOn": ["db-client:build"],
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/web"
      }
    }
  }
}
