{
  "name": "db-client",
  "sourceRoot": "libs/db-client/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "dist/libs/db-client",
        "main": "libs/db-client/src/index.ts",
        "tsConfig": "libs/db-client/tsconfig.lib.json",
        "assets": ["libs/db-client/*.md"],
        "generatePackageJson": true
      }
    },
    "generate": {
      "command": "pnpm drizzle-kit generate --config={projectRoot}/drizzle.config.ts",
      "options": { "cwd": "." }
    },
    "push": {
      "command": "pnpm drizzle-kit push --config={projectRoot}/drizzle.config.ts",
      "options": { "cwd": "." }
    },
    "migrate": {
      "command": "pnpm drizzle-kit migrate --config={projectRoot}/drizzle.config.ts",
      "options": { "cwd": "." }
    }
  }
}
